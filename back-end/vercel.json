{
  "version": 2,
  "builds": [
    {
      "src": "/back-end/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "/front-end/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/back-end/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/front-end/$1"
    }
  ]
}
