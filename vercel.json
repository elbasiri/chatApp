{
    "version": 2,
    "builds": [
      {
        "src": "test-chat-app-nj/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "test-chat-app-nj/app.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/test-chat-app-nj/app.js"
      }
    ]
  }